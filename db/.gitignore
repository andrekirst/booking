# Database - gitignore

# PostgreSQL specific
*.backup
*.bak
*.dump
*.sql.backup

# Database connection files
*.pgpass
.pgpass

# Logs
*.log
pg_log/
postgresql.log

# Temporary files
*.tmp
*.temp

# Database data files (should not be committed)
/data/
/pgdata/
/postgres-data/

# Backup files
/backups/
/dumps/

# Migration temporary files
*.migration.tmp
*.rollback.tmp

# Local database configuration
local.config
local.json
.env.local

# IDE generated files
.vscode/
.idea/

# OS generated files
.DS_Store
Thumbs.db

# Docker volumes (if using docker for database)
docker-compose.override.yml
